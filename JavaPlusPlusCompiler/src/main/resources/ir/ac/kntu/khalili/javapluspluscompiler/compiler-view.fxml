<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ir.ac.kntu.khalili.javapluspluscompiler.CompilerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Pane prefHeight="515.0" prefWidth="680.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <children>
            <TextArea fx:id="codeText" layoutX="14.0" layoutY="61.0" prefHeight="408.0" prefWidth="652.0" wrapText="true" />
            <Button layoutX="591.0" layoutY="14.0" mnemonicParsing="false" onAction="#onCompileButtonClick" text="Compile" />
         </children>
      </Pane>
      <Pane prefHeight="173.0" prefWidth="649.0">
         <children>
            <TextArea fx:id="compilationResultText" editable="false" prefHeight="159.0" prefWidth="680.0" promptText="Compilation Result" />
         </children>
      </Pane>
   </children>
</VBox>
